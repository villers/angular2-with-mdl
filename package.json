{
  "name": "Angular2Mdl",
  "version": "1.1.0",
  "description": "An Angular 2 Webpack Starter kit featuring Angular 2 with TypeScript MDL and Webpack",
  "author": "MickaÃ«l VILLERS <mickael.villers@epitech.eu>",
  "homepage": "https://github.com/villers/angular2-with-mdl",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/villers/angular2-with-mdl.git"
  },
  "bugs": {
    "url": "https://github.com/villers/angular2-with-mdl/issues"
  },
  "scripts": {
    "postinstall": "npm run typings-install",
    "start": "node ./node_modules/webpack-dev-server/bin/webpack-dev-server --inline --colors --progress --display-error-details --display-cached --port 8080 --config webpack.dev.config.js",
    "build": "node ./node_modules/webpack/bin/webpack --colors --progress --display-error-details -p --config webpack.prod.config.js --production",
    "clean": "node ./node_modules/rimraf/bin build typings doc",
    "docs": "node ./node_modules/typedoc/bin/typedoc --options typedoc.json --includeDeclarations typings/browser.d.ts src",
    "typings-install": "typings install",
    "typings-install-e": "typings install --ambient --save"
  },
  "devDependencies": {
    "appcache-webpack-plugin": "^1.2.1",
    "copy-webpack-plugin": "^0.3.3",
    "css-loader": "^0.23.1",
    "exports-loader": "^0.6.2",
    "file-loader": "^0.8.5",
    "html-webpack-plugin": "^1.7.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "node-sass": "^3.4.2",
    "on-build-webpack": "^0.1.0",
    "phantomjs": "^1.9.19",
    "phantomjs-polyfill": "0.0.1",
    "raw-loader": "^0.5.1",
    "rimraf": "^2.5.0",
    "sass-loader": "^3.1.2",
    "style-loader": "^0.13.0",
    "ts-loader": "^0.8.1",
    "tslint": "^3.5.0",
    "typedoc": "^0.3.12",
    "typescript": "^1.8.7",
    "typings": "^0.7.7",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.14",
    "webpack-dev-server": "^1.14.1"
  },
  "dependencies": {
    "angular2": "^2.0.0-beta.9",
    "es6-promise": "^3.1.2",
    "es6-shim": "^0.33.13",
    "material-design-icons": "^2.1.3",
    "material-design-lite": "^1.0.6",
    "reflect-metadata": "0.1.2",
    "rxjs": "^5.0.0-beta.2",
    "zone.js": "^0.5.15"
  }
}
